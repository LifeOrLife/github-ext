{
    "manifest_version": 2,
    "name": "github-ext",
    "version": "0.0.1",
    "description": "github extension",
    "icons": {
        "16": "bai.png",
        "32": "bai.png",
        "48": "bai.png",
        "64": "bai.png",
        "128": "bai.png"
    },
    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_icon": "bai.png",
        "default_title": "2333",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "github-ext.umd.js"
            ]
        }
    ],
    "permissions": [
        "activeTab",
        "tabs",
        "notifications",
        "webRequest",
        "webRequestBlocking",
        "storage"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "homepage_url": "https://www.bilibili.com"
}